        "def get_transcript(video_url):\n",
        "    try:\n",
        "        # Extract video ID from various URL formats\n",
        "        if \"watch?v=\" in video_url:\n",
        "            video_id = video_url.split(\"v=\")[-1].split(\"&\")[0]\n",
