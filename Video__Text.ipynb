      "source": [
        "# 📥 Step 2: Import required libraries\n",
        "from youtube_transcript_api import YouTubeTranscriptApi\n",
        "from transformers import pipeline\n",
        "import warnings\n",
        "warnings.filterwarnings(\"ignore\")\n"
      ],
      "metadata": {
        "id": "baYQuOeT3Dgk"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from youtube_transcript_api import YouTubeTranscriptApi\n",
        "\n",
        "def get_transcript(video_url):\n",
        "    try:\n",
        "        # Extract video ID\n",
        "        if \"watch?v=\" in video_url:\n",
        "            video_id = video_url.split(\"v=\")[-1].split(\"&\")[0]\n",
        "        elif \"youtu.be/\" in video_url:\n",
        "            video_id = video_url.split(\"/\")[-1].split(\"?\")[0]\n",
        "        else:\n",
        "            return \"Invalid YouTube URL\"\n",
        "\n",
        "        # ✅ Use instance method\n",
        "        transcript = YouTubeTranscriptApi().fetch(video_id)\n",
        "\n",
        "        full_text = \" \".join([x['text'] for x in transcript])\n",
        "        return full_text\n",
        "    except Exception as e:\n",
        "        return f\"Error fetching transcript: {str(e)}\"\n"
      ],
      "metadata": {
        "id": "-x9Bf9283UB3"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# 🧠 Step 4: Function to summarize transcript\n",
        "def summarize_text(text):\n",
        "    summarizer = pipeline(\"summarization\", model=\"facebook/bart-large-cnn\")\n",
        "\n",
        "    # Handling long text\n",
        "    max_chunk = 1000\n",
        "    text_chunks = [text[i:i+max_chunk] for i in range(0, len(text), max_chunk)]\n",
        "\n",
        "    summary = []\n",
        "    for chunk in text_chunks:\n",
        "        summary_text = summarizer(chunk, max_length=200, min_length=80, do_sample=False)[0]['summary_text']\n",
        "        summary.append(summary_text)\n",
        "\n",
        "    return \" \".join(summary)\n"
      ],
      "metadata": {
        "id": "84csNGpH3yEn"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "video_url = input(\"Paste YouTube URL: \")\n",
        "\n",
        "transcript = get_transcript(video_url)\n",
        "if \"Error\" not in transcript:\n",
        "    print(\"\\n📄 Transcript (First 1000 characters):\\n\")\n",
        "    print(transcript[:1000])\n",
        "\n",
        "    print(\"\\n🔍 Summarizing...\")\n",
        "    summary = summarize_text(transcript)\n",
        "\n",
        "    print(\"\\n📝 Summary:\\n\")\n",
        "    print(summary)\n",
        "else:\n",
        "    print(transcript)\n"
      ],
